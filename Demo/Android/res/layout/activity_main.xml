<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        tools:context=".MainActivity" >

        <TextView
            android:id="@+id/firstPerson"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfPersons"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/AEGBBuilding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfBuildings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstRoom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfRooms"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstGroupMembers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfGroups"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/secondAffiliationPersons"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfAffiliations"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstNewsItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfNewsItems"
            android:layout_width="wrap_content"
