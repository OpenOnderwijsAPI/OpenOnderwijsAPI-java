            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstCourseResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfCourseResults"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstTestResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfTestResults"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/firstMinor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />

        <TextView
            android:id="@+id/numberOfMinors"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@android:string/untitled" />
    </LinearLayout>

</ScrollView>